import java.util.*;

public class Palindrome {

public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A = sc.nextLine();
        
        String B = new StringBuilder(A).reverse().toString();
        
        //checks palidrome
        if (A.equals(B))
            System.out.print("Yes");
         else 
            System.out.println("No");

        
        
    }

}
